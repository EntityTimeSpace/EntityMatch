<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>实体查询</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>SegmentServlet</servlet-name>
    <servlet-class>servlet.SegmentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SegmentServlet</servlet-name>
  	<url-pattern>/Segment</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>GetClassTree</servlet-name>
    <servlet-class>servlet.GetClassTree</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetClassTree</servlet-name>
  	<url-pattern>/GetClassTree</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>AnnotateEntity</servlet-name>
  	<servlet-class>servlet.AnnotateEntity</servlet-class>
  </servlet><servlet-mapping>
  	<servlet-name>AnnotateEntity</servlet-name>
  	<url-pattern>/AnnotateEntity</url-pattern>
  </servlet-mapping>
	
	<servlet>
		<servlet-name>SegmentService</servlet-name>
		<servlet-class>servlet.SegmentService</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SegmentService</servlet-name>
		<url-pattern>/SegmentService</url-pattern>
	</servlet-mapping>
</web-app>